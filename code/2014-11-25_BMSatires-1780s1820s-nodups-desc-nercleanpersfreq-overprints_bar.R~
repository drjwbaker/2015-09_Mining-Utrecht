#load packages

setwd("/home/james/R/2014-11_BM-catalogue/data")
library(lubridate)
library(dplyr, warn.conflicts = FALSE)
library(tidyr, warn.conflicts = FALSE)
library(ggplot2)
library(dplyr)

#read data from csv

gender <- read.csv("2014-11-25_BMSatires-1780s1820s-nodups-desc-nercleanpersfreq.csv")

#plot data

ggplot(gender, aes(x = DECADE, y = (COUNT/TOTAL_PRINTS), fill = GENDER)) +
	geom_bar(stat = "identity") +
	coord_flip()

#save data to file

setwd("/home/james/R/2014-11_BM-catalogue/viz")
ggsave("2014-11-25_BMSatires-1780s1820s-nodups-desc-nercleanpersfreq-overprints.png", width = 7, height = 7)
